{
  "name": "your-project-name",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "jsx": { "version": 4, "mode": "classic" },
  "suffix": ".mjs",
  "bs-dependencies": ["@rescript/core", "@rescript/react"],
  "bsc-flags": ["-open RescriptCore"],
  "reanalyze": {
    "suppress": ["src/**/page/*.res"]
  }
}
